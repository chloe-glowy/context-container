{
  "compilerOptions": {
    "noEmitOnError": true,
    "sourceMap": true,
    "baseUrl": ".",
    "module": "commonjs",
    "outDir": "dist",
    "target": "es5",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "declaration": true
  },
  "skipLibCheck": true,
  "allowJs": false,
  "noImplicitAny": true
}
